/**app.wxss**/
/*主布局*/
.pageView{
	height: 100%;
	min-height: 100vh;
	box-sizing: border-box;
	border-top: solid 1rpx #e5e5e5;
    width: 100vw;
    display: flex;
    flex-direction: column;
    background-color: #F5F5F7;
	font-size: 30rpx;
	color: #333333;
}

.container {
  width: calc(100vw - 80rpx);
  margin: auto;
  height: 100%;
}

/**********************背景颜色**********************/
.bg_white {
	background-color: white;
}

/**********************弹窗**********************/
/*弹框遮罩*/
.dialog {
	width: 100vw;
	height: 100vh;
	position: fixed;
	top: 0;
	left: 0;
	display: flex;
	align-items: center;
	text-align: center;
	font-size: 30rpx;
	background: rgba(0, 0, 0, 0.3);
	z-index: 2;
}

/*弹框*/
.dialogView {
	background: #fff;
	width: 80vw;
	height: auto;
	min-height: 364;
    position: relative;
    left: 10vw;
    border-radius: 24rpx;
	display: flex;
	flex-direction: column;
}

/*弹框标题*/
.dialogView_title {
	width: 100%;
	height: 110rpx;
	line-height: 110rpx;
	border-bottom: solid 1rpx #e5e5e5;
	font-size: 36rpx;
}

/*弹框内容*/
.dialogView_content {
	width: calc(100% - 64rpx);
	height: auto;
	min-height: 160rpx;
	line-height: 160rpx;
	padding: 32rpx;
}

/*弹窗底部*/
.dialogView_bottom {
	width: 100%;
	height: 94rpx;
	display: flex;
	flex-direction: row;
	border-top: solid 1rpx #e5e5e5;
}

/*按钮*/
.dialogView_btn {
	flex: 1;
	width: 100%;
	height: 94rpx;
    line-height: 94rpx;
	color: #007AFF;
    font-size: 32rpx;
    text-align: center;
}
.dialogView_btn:nth-child(2) {
	border-left: solid 1rpx #e5e5e5;
}


/**********************列表样式**********************/
/*空列表*/
.emptyView {
	width: 100%;
    height: 100%;
    padding-top: 40%;
	text-align: center;
    font-size: 30rpx;
}

/**********************flex布局**********************/
/*左右排列*/
.flex_h {
	display: flex;
    flex-direction: row;
}

/*上下排列*/
.flex_v {
	display: flex;
    flex-direction: column;
}

/*垂直居中*/
.center_v {
    align-items: center;
}

/*水平居中*/
.center_h {
    justify-content: center;
}

/*居左*/
.align_left {
    text-align: left;
}

/*居右*/
.align_right {
    text-align: right;
}

/*flex平分*/
.flex_1 {
    flex: 1;
}

/*flex平分*/
.flex_2 {
    flex: 2;
}


/**********************文字相关**********************/
/*单行显示*/
.singleLine {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

/*文字居中*/
.text_center {
	line-height: 100%;
	text-align: center;
}

/*字体大小 最小26*/
.textSize_26 {
	font-size: 26rpx;
}

.textSize_30 {
	font-size: 30rpx;
}

.textSize_34 {
	font-size: 34rpx;
}

.textSize_48 {
	font-size: 48rpx;
}

/*字体颜色*/
.textColor_black {
	color: black;
}

.textColor_gray {
	color: #999999;
}

.textColor_blue {
	color: #3BB7F7;
}

.textColor_white {
	color: white;
}

/**********************基本布局**********************/
/*基本的itemView*/
.itemView {
	width: 100vw;
	height: 110rpx;
	display: flex;
	flex-direction: row;
	line-height: 110rpx;
	text-align: left;
}
.itemView:active {
	background-color: rgba(0, 0, 0, 0.1);
}
/*item左边的view*/
.item_leftView {
	width: calc(100% - 32rpx);
	padding-left: 32rpx;
}

/*右侧箭头*/
.iconRight {
	width: 16rpx;
    height: 26rpx;
	margin-top: auto;
	margin-bottom: auto;
	margin-left: 32rpx;
    padding-right: 30rpx;
}

/*底边框*/
.border_bottom {
	border-bottom: solid 1rpx #f1f1f1;
}

/**************线条相关**************/
/**竖线**/
.line_v {
    width: 1rpx;
    height: auto;
    background-color: #F1F1F1;
}

/**横线**/
.line_h {
 	width: auto;
    height: 1rpx;
    background-color: #F1F1F1;
}

/**********************基本按钮**********************/
.btn {
	width: 90vw;
	height: 94rpx;
	margin-left: 5vw;
	margin-top: 32rpx;
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
	font-size: 32rpx;
    color: white;
	border-radius: 8rpx;
	background: rgba(0, 0, 0, 0.3);
	box-shadow: 0 4rpx 10rpx 0 rgba(0, 0, 0, 0.3);
}

/*点击样式*/
.btn:active {
	background: rgba(0, 0, 0, 0.1);
}

/*红色按钮*/
.btn_red {
	background: #f05b48;
}

/*蓝色按钮*/
.btn_blue {
	background: #3BB7F7;
}

/*黑色按钮*/
.btn_black {
	background: #000000;
}

/**********************间隔样式**********************/
/*16*/
.mr_l_16 {margin-left: 16rpx;}
.mr_r_16 {margin-right: 16rpx;}
.mr_t_16 {margin-top: 16rpx;}

/*32*/
.mr_l_32 {margin-left: 32rpx;}
.mr_r_32 {margin-right: 32rpx;}
.mr_t_32 {margin-top: 32rpx;}

/*40*/
.mr_l_40 {margin-left: 40rpx;}
.mr_r_40 {margin-right: 40rpx;}
.mr_t_40 {margin-top: 40rpx;}

/*字体*/
@font-face {
    font-family: 'Bebas Neue';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2');
    font-weight: bold;
    font-style: normal;
}

/**********************公用样式**********************/
/*view标题部分*/
.cardTitle {
	width: 100%;
	height: 110rpx;
	line-height: 110rpx;
	display: flex;
	flex-direction: row;
	align-items: center;
}

.cardTitle .leftView,.rightView {
	flex: 1;
	height: 100%;
	line-height: 100%;
	display: flex;
	flex-direction: row;
	align-items: center;
}
.cardTitle .leftView .line {
	width: 6rpx;
	height: 30rpx;
	background: #2973FF;
	margin-right: 16rpx;
}

.cardTitle .rightView {
	justify-content: flex-end;
}
